@article{weiHistoricalReviewAntidepressant2020,
  title = {A Historical Review of Antidepressant Effects of Ketamine and Its Enantiomers},
  author = {Wei, Yan and Chang, Lijia and Hashimoto, Kenji},
  date = {2020-03},
  journaltitle = {Pharmacology Biochemistry and Behavior},
