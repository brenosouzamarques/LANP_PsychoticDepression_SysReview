@article{johnstoneCombinationTricyclicAntidepressant1990,
  title = {Combination Tricyclic Antidepressant and Lithium Maintenance Medication in Unipolar  and Bipolar Depressed Patients.},
  author = {Johnstone, E. C. and Owens, D. G. and Lambert, M. T. and Crow, T. J. and Frith, C. D. and Done, D. J.},
