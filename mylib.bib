  author = {Gelenberg, Alan J. and Trivedi, Madhukar H. and Rush, A. John and Thase, Michael E. and Howland, Robert and Klein, Daniel N. and Kornstein, Susan G. and Dunner, David L. and Markowitz, John C. and Hirschfeld, Robert M. A. and Keitner, Gabor I. and Zajecka, John and Kocsis, James H. and Russell, James M. and Miller, Ivan and Manber, Rachel and Arnow, Bruce and Rothbaum, Barbara and Munsaka, Melvin and Banks, Phillip and Borian, Frances E. and Keller, Martin B.},
  date = {2003-10-15},
  journaltitle = {Biological psychiatry},
